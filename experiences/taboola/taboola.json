{
	"cards": {
		"taboola": {
			"content": {
				"type": "AMPDocument",
				"url": "${AMP_DOCUMENT_URL}"
			},
			"snapPoints": {
				"initial": 0.7,
				"minimised": 0.85,
				"active": 80
			},
			"features": {
				"mode": "modal",
				"isDraggable": true,
				"button": {
					"action": "setSnapPoint:initial"
				},
				"useNaturalScroll": true,
				"header": {
					"dragHandlerBg": "#FFFFFF"
				}
			},
			"triggers": {
				"myScrollTrigger": {
					"on": "scroll",
					"spec": {
						"type": "absolute",
						"pixel": 300,
						"percentage": 0.1
					}
				}
			}
		}
	}
}
